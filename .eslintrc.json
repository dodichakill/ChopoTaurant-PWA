{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "quotes": "off",
    "indent": "off",
    "linebreak-style": "off",
    "arrow-parens": "off",
    "no-unused-vars": "off",
    "no-underscore-dangle": "off"
  }
}
